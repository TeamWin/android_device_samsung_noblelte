# Android fstab file.
#<src>                  <mnt_point>         <type>    <mnt_flags and options>                               <fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK
# data partition must be located at the bottom for supporting device encryption

/dev/block/platform/15570000.ufs/by-name/SYSTEM		/system		f2fs		ro																					wait
/dev/block/platform/15570000.ufs/by-name/SYSTEM		/system		ext4		ro,errors=panic																		wait
/dev/block/platform/15570000.ufs/by-name/USERDATA		/data		f2fs		nosuid,nodev,noatime,nodiratime,discard,inline_xattr											wait,encryptable=footer
/dev/block/platform/15570000.ufs/by-name/USERDATA		/data		ext4		noatime,nosuid,nodev,noauto_da_alloc,discard,journal_async_commit,data=ordered,errors=panic			wait,check,encryptable=footer
/dev/block/platform/15570000.ufs/by-name/CACHE		/cache		f2fs		nosuid,nodev,noatime,nodiratime,discard,inline_xattr											wait
/dev/block/platform/15570000.ufs/by-name/CACHE		/cache		ext4		noatime,nosuid,nodev,noauto_da_alloc,discard,journal_async_commit,data=ordered,errors=panic			wait,check
/dev/block/platform/15570000.ufs/by-name/PERSISTENT	/persistent	emmc		defaults																				defaults
/dev/block/platform/15570000.ufs/by-name/EFS			/efs			ext4		noatime,nosuid,nodev,noauto_da_alloc,discard,journal_async_commit,data=ordered,errors=panic			wait,check
/dev/block/platform/15570000.ufs/by-name/CPEFS		/cpefs		ext4		rw,errors=panic																		wait,check

# VOLD
/dev/block/platform/15570000.ufs/by-name/HIDDEN		/preload		ext4		defaults		voldmanaged=preload:auto

/devices/15400000.usb/15400000.dwc3/xhci-hcd.2.auto/usb*sda	auto		vfat		defaults		voldmanaged=UsbDriveA:auto
/devices/15400000.usb/15400000.dwc3/xhci-hcd.2.auto/usb*sdb	auto		vfat		defaults		voldmanaged=UsbDriveB:auto
/devices/15400000.usb/15400000.dwc3/xhci-hcd.2.auto/usb*sdc	auto		vfat		defaults		voldmanaged=UsbDriveC:auto
/devices/15400000.usb/15400000.dwc3/xhci-hcd.2.auto/usb*sdd	auto		vfat		defaults		voldmanaged=UsbDriveD:auto
/devices/15400000.usb/15400000.dwc3/xhci-hcd.2.auto/usb*sde	auto		vfat		defaults		voldmanaged=UsbDriveE:auto
/devices/15400000.usb/15400000.dwc3/xhci-hcd.2.auto/usb*sdf	auto		vfat		defaults		voldmanaged=UsbDriveF:auto
